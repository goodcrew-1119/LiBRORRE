<% stories.each do |story| %>
  <div class="story-list clearfix">
    <h4><%= link_to story.title, story_path(story) %> / <%= link_to story.user.name, user_path(story.user) %></h4>
    <p>
      <%= story.created_at.strftime("%Y - %-m - %-d / %-H : %-M") %>/
      ライブラリ登録数：<%= story.libraries.count %>
    </p>
  </div>
<% end %>