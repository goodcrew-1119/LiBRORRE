<% libraries.each do |library| %>
<% stock = library.story %>
<div class="story-list clearfix">
  <p><%= link_to stock.title, story_path(stock) %> / <%= link_to stock.user.name, user_path(stock.user) %></p>
  <p>
    <%= stock.created_at.strftime("%Y - %-m - %-d / %-H : %-M") %>/
    ライブラリ登録数：<%= stock.libraries.count %>
  </p>
</div>
<% end %>